{
  "name": "mercado-liebre-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "nodemon ./src/bin/www",
    "create:db": "sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "reset:db": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "dev:reset":"npm run reset:db && nodemon ./src/bin/www",
    "start": "npm run reset:db && node ./src/bin/www"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.2",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.22.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.1",
    "sequelize-cli": "^6.5.1"
  }
}
